{% extends 'base.html.twig' %}

{% block title %}Hello SecurityController!{% endblock %}

{% block body %}

<div class="container">
    <h1>Inscription</h1>
    {{ form_start(form) }}

    {{ form_row(form.email) }}
    {{ form_row(form.username) }}
    {{ form_row(form.password) }}
    {{ form_row(form.confirmPassword) }}

    <button class="btn btn-primary">Inscription</button>

    {{ form_end(form) }}


</div>
{% endblock %}
